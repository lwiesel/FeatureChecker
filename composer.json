{
    "name": "lwiesel/feature-checker",
    "description": "Enable and disable functional features in php application.",
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1",
        "scrutinizer/ocular": "~1.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Laurent Wiesel",
            "email": "wiesel.laurent@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "LWI\\FeatureChecker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LWI\\FeatureChecker\\Stub\\": "stub/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
