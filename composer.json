{
    "name": "lwiesel/feature-checker",
    "description": "Enable and disable functional features in php application.",
    "keywords": ["feature-checker", "FeatureChecker"],
    "homepage": "https://github.com/lwiesel/FeatureChecker",
    "license": "MIT",
    "authors": [
        {
            "name": "Laurent Wiesel",
            "email": "wiesel.laurent@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1",
        "henrikbjorn/phpspec-code-coverage": "~1.0",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "LWI\\FeatureChecker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LWI\\FeatureChecker\\Test\\": "tests"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
